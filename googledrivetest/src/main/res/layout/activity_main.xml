<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ru.pnapp.googledrivetest.MainActivity"
    tools:ignore="HardcodedText">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/activity_result"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Activity result"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/set_enabled"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Set enabled"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/connect_disabled"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Connect fails if not enabled"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/connect"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Connect"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/cd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Change dir"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/write"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Write"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/review_create"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Create new file"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/stream_write"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Stream write "
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/stream_read"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Stream read"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/ls"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="List files"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/last_modofied"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Get lats modified"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/delete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_help_black_24dp"
            android:drawableRight="@drawable/ic_help_black_24dp"
            android:text="Delete files"
            android:textSize="18sp"/>

    </LinearLayout>

</ScrollView>
